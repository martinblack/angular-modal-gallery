{
  "name": "@ks89/angular-modal-gallery",
  "version": "7.0.0",
  "description": "Modal image gallery for Angular >=2",
  "license": "MIT",
  "author": "Stefano Cappa",
  "homepage": "https://github.com/Ks89",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Ks89/angular-modal-gallery.git"
  },
  "engines": {
    "node": ">=10.11.3",
    "npm": ">=6.4.1"
  },
  "scripts": {
    "ng": "ng",
    "start": "npm run serve",
    "serve": "npm run serve:dev",
    "serve:dev": "ng serve --aot",
    "serve:prod": "ng serve --prod  --aot --build-optimizer=true",
    "build": "npm run build:lib && npm run build:main",
    "build:lib": "ng build @ks89/angular-modal-gallery && npm run copy:build && npm run copy:other",
    "build:lib:watch": "ng build @ks89/angular-modal-gallery --watch",
    "build:main": "npm run build:main:dev",
    "build:main:dev": "ng build --aot",
    "build:main:prod": "ng build --prod --aot --build-optimizer=true",
    "build:examples": "npm run build:lib && npm run clean:examples && npm run copy:dist:examples",
    "build:all": "npm run build:lib && npm run build:main && npm run build:examples",
    "clean": "npm run clean:main",
    "clean:main": "npm run clean:dist && npm run clean:coverage && npm run clean:docs && npm run clean:sonar",
    "clean:dist": "rimraf @ks89",
    "clean:coverage": "rimraf coverage",
    "clean:docs": "rimraf docs",
    "clean:sonar": "rimraf reports .scannerwork",
    "clean:systemjs": "rimraf examples/systemjs/node_modules/@ks89",
    "clean:angular-cli": "rimraf examples/angular-cli/node_modules/@ks89 examples/angular-cli/dist",
    "clean:angular-cli-6": "rimraf examples/angular-cli-6/node_modules/@ks89 examples/angular-cli-6/dist",
    "clean:angular-cli-material": "rimraf examples/angular-cli-material/node_modules/@ks89 examples/angular-cli-material/dist",
    "clean:universal": "rimraf examples/universal/node_modules/@ks89 examples/universal/dist",
    "clean:examples": "npm run clean:systemjs && npm run clean:angular-cli && npm run clean:angular-cli-6 && npm run clean:angular-cli-material && npm run clean:universal",
    "clean:all": "npm run clean:main && npm run clean:examples",
    "copy:dist": "npm run copy:dist:examples",
    "copy:dist:systemjs": "cpr @ks89 examples/systemjs/node_modules/@ks89 -o",
    "copy:dist:angular-cli": "cpr @ks89 examples/angular-cli/node_modules/@ks89 -o",
    "copy:dist:angular-cli-6": "cpr @ks89 examples/angular-cli-6/node_modules/@ks89 -o",
    "copy:dist:angular-cli-material": "cpr @ks89 examples/angular-cli-material/node_modules/@ks89 -o",
    "copy:dist:universal": "cpr @ks89 examples/universal/node_modules/@ks89 -o",
    "copy:dist:examples": "npm run copy:dist:systemjs && npm run copy:dist:angular-cli && npm run copy:dist:angular-cli-6 && npm run copy:dist:angular-cli-material && npm run copy:dist:universal",
    "copy:build": "cpr dist/ks89/angular-modal-gallery @ks89/angular-modal-gallery -o",
    "copy:other": "cpr README.md @ks89/angular-modal-gallery/README.md -o && cpr LICENSE @ks89/angular-modal-gallery/LICENSE -o && cpr CHANGELOG.md @ks89/angular-modal-gallery/CHANGELOG.md -o && cpr CONTRIBUTING.md @ks89/angular-modal-gallery/CONTRIBUTING.md -o",
    "pretest": "npm run clean:coverage && npm run lint",
    "test": "ng test @ks89/angular-modal-gallery --watch=false --code-coverage=true",
    "test:snyk": "snyk test && npm run test",
    "test:watch": "ng test @ks89/angular-modal-gallery --watch=true --code-coverage=true",
    "test:ci": "ng test @ks89/angular-modal-gallery --watch=false --no-progress --code-coverage=true",
    "test:ci:jenkins": "cross-env-shell CI=true JENKINS=true ng test @ks89/angular-modal-gallery --watch=false --no-progress --code-coverage=true",
    "e2e": "ng e2e",
    "e2e:ee": "npm run e2e -- --element-explorer=true",
    "e2e:ci": "npm run e2e -- --protractor-config=e2e/protractor-ci.conf.js",
    "lint": "ng lint",
    "docs": "npm run docs:typedoc && npm run docs:compodoc",
    "docs:typedoc": "typedoc --out ./docs/typedoc --mode file --target ES6 --exclude **/*.spec.ts ./projects/ks89/angular-modal-gallery",
    "docs:typedoc:serve": "npm run docs:typedoc && lite-server --baseDir=\"docs/typedoc\"",
    "docs:compodoc": "compodoc projects/ks89/angular-modal-gallery -p tsconfig.json -d docs/compodoc",
    "docs:compodoc:serve": "npm run docs:compodoc -- -s",
    "docs:compodoc:serve:watch": "npm run docs:compodoc -- -s -w",
    "analyze:sonar": "npm run clean && npm test && sonar-scanner -Dsonar.projectKey=angular-modal-gallery -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "analyze:sonar:ci": "npm run clean && npm run test:ci && sonar-scanner -Dsonar.projectKey=angular-modal-gallery -Dsonar.sources=. -Dsonar.host.url=https://sonarcloud.io -Dsonar.organization=ks89-github",
    "check:licenses": "license-checker --exclude 'MIT, MIT OR X11, BSD, ISC, Apache-2.0'",
    "coveralls": "coveralls < coverage/lcov.info",
    "codeclimate": "codeclimate-test-reporter < coverage/lcov.info"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged --verbose"
    }
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.0",
    "@angular/cdk": "^7.2.1",
    "@angular/common": "^7.2.0",
    "@angular/compiler": "^7.2.0",
    "@angular/core": "^7.2.0",
    "@angular/forms": "^7.2.0",
    "@angular/http": "^7.2.0",
    "@angular/platform-browser": "^7.2.0",
    "@angular/platform-browser-dynamic": "^7.2.0",
    "@angular/router": "^7.2.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.8",
    "@fortawesome/free-solid-svg-icons": "^5.5.0",
    "core-js": "^2.6.2",
    "hammerjs": "^2.0.8",
    "mousetrap": "^1.6.2",
    "rxjs": "~6.3.3",
    "zone.js": "^0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.12.1",
    "@angular-devkit/build-ng-packagr": "^0.12.1",
    "@angular/cli": "^7.2.1",
    "@angular/compiler-cli": "^7.2.0",
    "@angular/language-service": "^7.2.0",
    "@compodoc/compodoc": "^1.1.7",
    "@types/hammerjs": "^2.0.36",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "^2.0.6",
    "@types/mousetrap": "^1.6.0",
    "@types/node": "^10.12.12",
    "codeclimate-test-reporter": "^0.5.0",
    "codelyzer": "~4.5.0",
    "coveralls": "^3.0.1",
    "cpr": "^3.0.1",
    "cross-env": "^5.2.0",
    "husky": "^1.2.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^3.1.3",
    "karma-chrome-launcher": "~2.2.0",
    "karma-cli": "~1.0.1",
    "karma-coverage": "^1.1.2",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-edge-launcher": "^0.4.2",
    "karma-firefox-launcher": "^1.1.0",
    "karma-ie-launcher": "^1.0.0",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-mocha-reporter": "^2.2.5",
    "karma-safari-launcher": "^1.0.0",
    "karma-sonarqube-unit-reporter": "0.0.18",
    "lite-server": "^2.3.0",
    "ng-packagr": "^4.6.0",
    "prettier": "=1.14.3",
    "pretty-quick": "=1.8.0",
    "protractor": "~5.4.1",
    "rimraf": "^2.6.2",
    "sass-lint": "^1.12.1",
    "snyk": "^1.114.0",
    "sonar-scanner": "^3.0.3",
    "ts-node": "~7.0.0",
    "tsickle": "^0.34.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typedoc": "^0.13.0-0",
    "typescript": "~3.1.1"
  }
}
